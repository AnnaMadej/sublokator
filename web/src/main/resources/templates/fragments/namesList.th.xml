<thLogic>
    <attr sel="#namesListFragment" th:fragment="namesListFragment"/>
    <attr sel="#namesList" th:each="name : ${__${T(com.aniamadej.sublokator.util.Attributes).NAMES}__}">
        <attr sel=".namesListEntry"
              th:href="${__${T(com.aniamadej.sublokator.util.Attributes).REDIRECT_PAGE}__} + '/' +   ${name.getId}"
              th:text="${name.getName()}"/>

    </attr>
</thLogic>
